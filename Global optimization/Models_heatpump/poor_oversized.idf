EnergyManagementSystem:Program,
    heatCoolAvailPG,         !- Name
    if officeZAT < (officeHtgSp - 0.5),  !- Program Line 1
    set officeHeatAvail = 1, !- Program Line 2
    elseif officeZAT > (officeHtgSp + 0.5),  !- A4
    set officeHeatAvail = 0, !- A5
    else,                    !- A6
    set officeHeatAvail = 0, !- A7
    endif,                   !- A8
    if officeZAT > (officeClgSp + 0.5),  !- A9
    set officeCoolAvail = 1, !- A10
    elseif officeZAT < (officeClgSp - 0.5),  !- A11
    set officeCoolAvail = 0, !- A12
    else,                    !- A13
    set officeCoolAvail = 0, !- A14
    endif,                   !- A15
    if lobbyZAT < (lobbyHtgSp - 0.5),  !- A16
    set lobbyHeatAvail = 1,  !- A17
    elseif lobbyZAT > (lobbyHtgSp + 0.5),  !- A18
    set lobbyHeatAvail = 0,  !- A19
    else,                    !- A20
    set lobbyHeatAvail = 0,  !- A21
    endif,                   !- A22
    if lobbyZAT > (lobbyClgSp + 0.5),  !- A23
    set lobbyCoolAvail = 1,  !- A24
    elseif lobbyZAT < (lobbyClgSp - 0.5),  !- A25
    set lobbyCoolAvail = 0,  !- A26
    else,                    !- A27
    set lobbyCoolAvail = 0,  !- A28
    endif,                   !- A29
    if electricalZAT < (electricalHtgSp - 0.5),  !- A30
    set electricalHeatAvail = 1,  !- A31
    elseif electricalZAT > (electricalHtgSp + 0.5),  !- A32
    set electricalHeatAvail = 0,  !- A33
    else,                    !- A34
    set electricalHeatAvail = 0,  !- A35
    endif,                   !- A36
    if electricalZAT > (electricalClgSp + 0.5),  !- A37
    set electricalCoolAvail = 1,  !- A38
